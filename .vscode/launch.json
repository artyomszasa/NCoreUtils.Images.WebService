{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-gcr",
            "program": "${workspaceFolder}/NCoreUtils.Images.WebService.GoogleCloudRun/bin/Debug/net6.0/NCoreUtils.Images.WebService.GoogleCloudRun.dll",
            "args": [],
            "cwd": "${workspaceFolder}/NCoreUtils.Images.WebService.GoogleCloudRun",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "GOOGLE_APPLICATION_CREDENTIALS": "/etc/google/wlb-2018.json"
            },
            "suppressJITOptimizations": true
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "name": "Attach to .NET Functions",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:azureFunctions.pickProcess}"
        }
    ]
}